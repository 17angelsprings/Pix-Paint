; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="ui"
run/main_scene="res://src/ui/menu/menu.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://assets/brush.png"

[autoload]

ToolGlobals="*res://src/autoload/tool_globals.gd"
CanvasGlobals="*res://src/autoload/canvas_globals.gd"
FileGlobals="*res://src/autoload/file_globals.gd"

[display]

window/size/viewport_width=1365
window/size/viewport_height=750
window/subwindows/embed_subwindows=false
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
